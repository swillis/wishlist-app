<!DOCTYPE html>
<html>
  <head>
    <title>Wishlist</title>
    
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%=render 'layouts/header'%>
    <div class='row'>
       <p id="notice" class="notice" ><%= notice %></p>
       <p id="alert" class="alert"><%= alert %></p>
      <%= yield %>
    </div>
    
  </body>
</html>
